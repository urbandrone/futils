{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["src", "package.json", "readme_jsdoc.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|specs/|docs/|dist/)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "./docsrc/_tpl/minami",
        "tutorials": "./docsrc/tutorials"
    }
}