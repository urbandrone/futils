{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["sources", "package.json", "readme_jsdoc.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|specs/|docs/|dist/)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./docs/_tpl/minami"
    }
}