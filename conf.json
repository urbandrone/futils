{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["src", "package.json"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|specs/|docs/|dist/)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "./docsrc/template/futils",
        "readme": "./docsrc/home.md",
        "tutorials": "./docsrc/tutorials"
    },
    "templates": {
        "default": {
            "staticFiles": {
                "include": [
                    "./docsrc/template/static"
                ]
            }
        }
    }
}